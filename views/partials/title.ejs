<div id="title" class="title">
  <H1 class="text-center text-white pt-3 outlined-text display-1 fw-bold"
    ><%= title %></H1
  >
  <h5 class="card-title fs-2 outlined-text mx-2" id="datetime"></h5>
  <div
    class="d-flex justify-content-between align-items-center text-white outlined-text fs-3 fw-bold mt-1 mb-0 me-2"
  >
    <span class="card-text mb-0 ms-2 mt-" id="city"></span>
    <span class="card-text mb-0"><img id="clouds" src="" alt="" /></span>
    <span class="card-text mb-0" id="temp"></span>
  </div>
</div>
